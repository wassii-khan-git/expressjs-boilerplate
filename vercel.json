{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "PORT_NUMBER": "@port_number",
    "JWT_SECRET": "@jwt_secret"
  }
}
